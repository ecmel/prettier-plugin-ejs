{
  "name": "prettier-plugin-ejs",
  "displayName": "Prettier EJS Plugin",
  "version": "1.0.3",
  "description": "A Prettier plugin for Embedded JavaScript Templating (EJS).",
  "keywords": [
    "prettier",
    "plugin",
    "ejs",
    "html"
  ],
  "author": {
    "name": "Ecmel Ercan",
    "url": "https://ecmel.dev/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ecmel/prettier-plugin-ejs.git"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "clean": "git clean -Xdf",
    "format": "prettier --ignore-path=.gitignore --plugin=./index.js --write ."
  },
  "devDependencies": {
    "prettier": "^2.8.8"
  },
  "peerDependencies": {
    "prettier": "2.x - 3.x"
  }
}
